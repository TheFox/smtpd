{
	"name": "thefox/smtpd",
	"description": "SMTP server (library) written in pure PHP.",
	"license": "GPL-3.0",
	"type": "library",
	"keywords": [ "smtp", "email", "mail", "server", "daemon" ],
	"homepage": "https://fox21.at",
	"authors": [
		{
			"name": "Christian Mayer",
			"email": "christian@fox21.at",
			"homepage": "https://fox21.at"
		}
	],
	"require": {
		"php": "^5.3 || ^7.0",
		"ext-sockets": ">=0",
		"zendframework/zend-mail": "^2.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8",
		"phpmailer/phpmailer": "^5.2",
		"phpstan/phpstan": "^0.7",
		"thefox/utilities": "^1.1"
	},
	"autoload": {
		"psr-0": { "TheFox": "src" }
	},
	"autoload-dev": {
		"psr-0": { "TheFox": "tests" }
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x.x-dev"
		}
	}
}
